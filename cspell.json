{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": ["pnpm-lock.yaml", "**.svg", ".git/", ".gitignore", ".vscode/", "./patches/"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"codeowner",
		"pawelblaszczyk",
		"stylesheet",
		"tokenami",
		"typecheck",
		"unopinionated"
	],
	"enableGlobDot": true,
	"enableFiletypes": ["dockerfile", "github-actions-workflow"],
	"ignoreWords": ["commonalityco", "isbot", "jsxdev", "nuqs", "pipeable", "replicache", "taze", "tseslint", "xyflow"]
}
